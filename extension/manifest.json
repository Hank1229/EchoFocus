{
  "manifest_version": 3,
  "name": "Productivity Tracker",
  "version": "1.0.0",
  "description": "Tracking your productivity by AI powered monitoring",
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "http://localhost:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
